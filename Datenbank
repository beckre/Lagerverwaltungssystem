```ruby
CREATE TABLE `Artikel` 
(
 `id` varchar(20) COLLATE utf8mb4_unicode_ci NOT NULL,
 `beschreibung` varchar(50) COLLATE utf8mb4_unicode_ci NOT NULL,
 `hersteller` varchar(50) COLLATE utf8mb4_unicode_ci NOT NULL,
 PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci

CREATE TABLE `Logbuch` 
(
 `regal` varchar(20) COLLATE utf8mb4_unicode_ci NOT NULL COMMENT 'references regal(id)',
 `zugriff` mediumint(9) unsigned NOT NULL,
 PRIMARY KEY (`regal`),
 CONSTRAINT `Logbuch_ibfk_1` FOREIGN KEY (`regal`) REFERENCES `Regal` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci

CREATE TABLE `Regal` (
 `id` varchar(20) COLLATE utf8mb4_unicode_ci NOT NULL COMMENT 'position (R: row number, C: column number)',
 `inhalt` varchar(20) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT 'references Artikel (id)',
 PRIMARY KEY (`id`),
 KEY `inhalt` (`inhalt`),
 CONSTRAINT `Regal_ibfk_1` FOREIGN KEY (`inhalt`) REFERENCES `Artikel` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci
```
